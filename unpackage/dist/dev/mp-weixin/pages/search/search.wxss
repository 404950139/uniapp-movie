.poster {
	width:200rpx;
	height:270rpx;
}
.movie-list {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap:wrap;
	    -ms-flex-wrap:wrap;
	        flex-wrap:wrap;
	padding:100rpx 10rpx 0 10rpx;
	
}
.movie-wrapper {
	padding:10rpx -webkit-calc(130rpx / 6);
	padding:10rpx calc(130rpx / 6);
}

